{
  "hosting": {
    "public": "dist/apps/gps",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "targets": {
      "gpsdev": {
        "hosting": {
          "public": "dist/apps/gps",
          "rewrites": [
            {
              "source": "**",
              "destination": "/index.html"
            }
          ]
        }
      }
    }
  }
}
